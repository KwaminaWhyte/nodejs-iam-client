# Adamus IAM Client Configuration

# Required: Base URL of your IAM service
IAM_BASE_URL=http://localhost:8000/api/v1

# Optional: Request timeout in milliseconds (default: 10000)
IAM_TIMEOUT=10000

# Optional: Verify SSL certificates (default: true)
IAM_VERIFY_SSL=true

# For React applications, prefix with REACT_APP_
# REACT_APP_IAM_BASE_URL=http://localhost:8000/api/v1
# REACT_APP_IAM_TIMEOUT=10000
# REACT_APP_IAM_VERIFY_SSL=true
